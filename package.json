{
  "name": "whocraft-launcher",
  "version": "1.0.0",
  "description": "Launcher Minecraft Whocraft",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "pack": "electron-packager . WhoCraftLauncher --platform=win32 --arch=x64 --out=dist --overwrite"
  },
  "keywords": [],
  "author": "Manasei",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-packager": "^17.1.2",
    "electron-updater": "^6.7.3"
  },
  "dependencies": {
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "prismarine-nbt": "^2.8.0",
    "ws": "^8.19.0"
  },
  "build": {
    "appId": "fr.whocraft.launcher",
    "productName": "Whocraft Launcher",
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Manasei",
        "repo": "whocraft-launcher"
      }
    ]
  }
